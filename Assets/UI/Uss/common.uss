#root {
    position: relative;
    width: 100%;
    height: 100%;
    background-color: brown;
    -unity-text-align: middle-center;
}

#border {
    position: absolute;
    width: 90%;
    height: 60%;
    top: 20%;
    left: 5%;
    border-width: 10px;
    border-radius: 48px;
    border-color: white;
}